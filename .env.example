# Qwen LLM
QWEN_API_KEY=your-qwen-api-key-here
QWEN_API_BASE=http://localhost:8001/v1
QWEN_MODEL=qwen-max

# Qwen VLM
QWEN_VL_API_KEY=your-qwen-vl-api-key-here
QWEN_VL_API_BASE=http://localhost:8000/v1
QWEN_VL_MODEL=qwen-vl-plus

